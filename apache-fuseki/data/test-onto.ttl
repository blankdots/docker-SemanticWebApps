                                                        rdfs:range <http://blankdots.com/open/personasonto.owl#Context> ;

                                                        rdfs:domain <http://blankdots.com/open/personasonto.owl#Scenario> .



###  http://blankdots.com/open/personasonto.owl#hasDisability

<http://blankdots.com/open/personasonto.owl#hasDisability> rdf:type :ObjectProperty ;

                                                           dc:description "A person might have a disability." ;

                                                           rdfs:range <http://blankdots.com/open/personasonto.owl#Disability> ;

                                                           rdfs:domain <http://blankdots.com/open/personasonto.owl#Person> .



###  http://blankdots.com/open/personasonto.owl#hasEndGoal

<http://blankdots.com/open/personasonto.owl#hasEndGoal> rdf:type :ObjectProperty ;

                                                        rdfs:range <http://blankdots.com/open/personasonto.owl#EndGoals> ;

                                                        rdfs:subPropertyOf <http://blankdots.com/open/personasonto.owl#hasGoal> .



###  http://blankdots.com/open/personasonto.owl#hasExperienceGoal

<http://blankdots.com/open/personasonto.owl#hasExperienceGoal> rdf:type :ObjectProperty ;

                                                               rdfs:range <http://blankdots.com/open/personasonto.owl#ExperienceGoals> ;

                                                               rdfs:subPropertyOf <http://blankdots.com/open/personasonto.owl#hasGoal> .



###  http://blankdots.com/open/personasonto.owl#hasGoal

<http://blankdots.com/open/personasonto.owl#hasGoal> rdf:type :ObjectProperty ;

                                                     dc:description "A person might have one or more (short/long term) goals depending on the context." ;

                                                     rdfs:range <http://blankdots.com/open/personasonto.owl#Goals> ;

                                                     rdfs:domain <http://blankdots.com/open/personasonto.owl#Person> ;

                                                     :inverseOf <http://blankdots.com/open/personasonto.owl#isGoalOf> .



###  http://blankdots.com/open/personasonto.owl#hasInfluence

<http://blankdots.com/open/personasonto.owl#hasInfluence> rdf:type :ObjectProperty ;

                                                          dc:description "The disability of a person migh have an effect on the affective state or personality on the person." ;

                                                          rdfs:domain <http://blankdots.com/open/personasonto.owl#Disability> ;

                                                          rdfs:range [ rdf:type :Class ;
                                                                       :unionOf ( <http://blankdots.com/open/personasonto.owl#AffectiveState>
                                                                                  <http://blankdots.com/open/personasonto.owl#Personality>
                                                                                )
                                                                     ] .



###  http://blankdots.com/open/personasonto.owl#hasInterest

<http://blankdots.com/open/personasonto.owl#hasInterest> rdf:type :ObjectProperty ;

                                                         dc:description "A person might be interested in a certain resource (product, application, document)." ;

                                                         rdfs:domain <http://blankdots.com/open/personasonto.owl#Person> ;

                                                         rdfs:range <http://blankdots.com/open/personasonto.owl#Resource> .



###  http://blankdots.com/open/personasonto.owl#hasLifeGoal

<http://blankdots.com/open/personasonto.owl#hasLifeGoal> rdf:type :ObjectProperty ;

                                                         rdfs:range <http://blankdots.com/open/personasonto.owl#LifeGoals> ;

                                                         rdfs:subPropertyOf <http://blankdots.com/open/personasonto.owl#hasGoal> .



###  http://blankdots.com/open/personasonto.owl#hasPersonaType

<http://blankdots.com/open/personasonto.owl#hasPersonaType> rdf:type :ObjectProperty ;

                                                            dc:description "The type of Persona: Primary, Secondary, Negative, Supplemental, Served or Customer." ;

                                                            rdfs:range <http://blankdots.com/open/personasonto.owl#PersonaType> ;

                                                            rdfs:domain [ rdf:type :Class ;
                                                                          :unionOf ( <http://blankdots.com/open/personasonto.owl#Participant>
                                                                                     <http://blankdots.com/open/personasonto.owl#Person>
                                                                                   )
                                                                        ] .



###  http://blankdots.com/open/personasonto.owl#hasPersonality

<http://blankdots.com/open/personasonto.owl#hasPersonality> rdf:type :ObjectProperty ;

                                                            dc:description "A person has a certain personality corresponding to Myers Briggs personality classification." ;

                                                            rdfs:domain <http://blankdots.com/open/personasonto.owl#Person> ;

                                                            rdfs:range <http://blankdots.com/open/personasonto.owl#Personality> .



###  http://blankdots.com/open/personasonto.owl#hasPublications

<http://blankdots.com/open/personasonto.owl#hasPublications> rdf:type :ObjectProperty ;

                                                             dc:description "A person (of academic, journalistic profession) has published several documents." ;

                                                             rdfs:range <http://blankdots.com/open/personasonto.owl#Document> ;

                                                             rdfs:domain <http://blankdots.com/open/personasonto.owl#Person> .



###  http://blankdots.com/open/personasonto.owl#hasReview

<http://blankdots.com/open/personasonto.owl#hasReview> rdf:type :ObjectProperty ;

                                                       dc:description "A resource, scenario or usability test is described, reviewed via a document." ;

                                                       rdfs:range <http://blankdots.com/open/personasonto.owl#Document> ;

                                                       rdfs:domain [ rdf:type :Class ;
                                                                     :unionOf ( <http://blankdots.com/open/personasonto.owl#Resource>
                                                                                <http://blankdots.com/open/personasonto.owl#Scenario>
                                                                                <http://blankdots.com/open/personasonto.owl#UsabilityTest>
                                                                              )
                                                                   ] .



###  http://blankdots.com/open/personasonto.owl#hasTask

<http://blankdots.com/open/personasonto.owl#hasTask> rdf:type :ObjectProperty ;

