                                                                               :someValuesFrom <http://blankdots.com/open/personasonto.owl#Personality>
                                                                             ] ;

                                                            rdfs:subClassOf :Thing ;

                                                            dc:description "The affective state of a person at a certain moment and in a certain Context." .



###  http://blankdots.com/open/personasonto.owl#BusinessGoals

<http://blankdots.com/open/personasonto.owl#BusinessGoals> rdf:type :Class ;

                                                           rdfs:subClassOf <http://blankdots.com/open/personasonto.owl#Goals> ;

                                                           dc:description "Business goals represent the goals of the organization the persons works for." .



###  http://blankdots.com/open/personasonto.owl#Context

<http://blankdots.com/open/personasonto.owl#Context> rdf:type :Class ;

                                                     rdfs:subClassOf :Thing ;

                                                     dc:description "The Context in which a scenario takes place." .



###  http://blankdots.com/open/personasonto.owl#Corporation

<http://blankdots.com/open/personasonto.owl#Corporation> rdf:type :Class ;

                                                         rdfs:subClassOf <http://blankdots.com/open/personasonto.owl#Organization> ;

                                                         dc:description "A business corporation. " .



###  http://blankdots.com/open/personasonto.owl#CustomerType

<http://blankdots.com/open/personasonto.owl#CustomerType> rdf:type :Class ;

                                                          rdfs:subClassOf <http://blankdots.com/open/personasonto.owl#PersonaType> ;

                                                          dc:description "Are not the end user but the customer of the product, but might be the person making, buying decisions." .



###  http://blankdots.com/open/personasonto.owl#DevelopmentDisability

<http://blankdots.com/open/personasonto.owl#DevelopmentDisability> rdf:type :Class ;

                                                                   rdfs:subClassOf <http://blankdots.com/open/personasonto.owl#Disability> ;

                                                                   dc:description "Any disability that related to growth and development problems." .



###  http://blankdots.com/open/personasonto.owl#Disability

<http://blankdots.com/open/personasonto.owl#Disability> rdf:type :Class ;

                                                        rdfs:subClassOf :Thing ;

                                                        dc:description "A person’s disabilities mental and/or physical." .



###  http://blankdots.com/open/personasonto.owl#Document

<http://blankdots.com/open/personasonto.owl#Document> rdf:type :Class ;

                                                      rdfs:subClassOf <http://blankdots.com/open/personasonto.owl#Resource> ;

                                                      dc:description "A paper or digital document (web-page, text document etc.)." .



###  http://blankdots.com/open/personasonto.owl#EducationalOrganization

<http://blankdots.com/open/personasonto.owl#EducationalOrganization> rdf:type :Class ;

                                                                     rdfs:subClassOf <http://blankdots.com/open/personasonto.owl#Organization> ;

                                                                     dc:description "An educational organization. " .



###  http://blankdots.com/open/personasonto.owl#Emotion

<http://blankdots.com/open/personasonto.owl#Emotion> rdf:type :Class ;

                                                     rdfs:subClassOf <http://blankdots.com/open/personasonto.owl#AffectiveState> ;

                                                     dc:description "Emotion encompasses physiological, affective, behavioral, andcognitive components." .



###  http://blankdots.com/open/personasonto.owl#EndGoals

<http://blankdots.com/open/personasonto.owl#EndGoals> rdf:type :Class ;

                                                      rdfs:subClassOf <http://blankdots.com/open/personasonto.owl#Goals> ;

                                                      dc:description "End goals represent the user’s motivation for performing the tasks associated with using a specific product." .



###  http://blankdots.com/open/personasonto.owl#ExperienceGoals

<http://blankdots.com/open/personasonto.owl#ExperienceGoals> rdf:type :Class ;

                                                             rdfs:subClassOf <http://blankdots.com/open/personasonto.owl#Goals> ;

                                                             dc:description "Experience goals are simple, universal, and personal." .



###  http://blankdots.com/open/personasonto.owl#Feeling

<http://blankdots.com/open/personasonto.owl#Feeling> rdf:type :Class ;

                                                     rdfs:subClassOf <http://blankdots.com/open/personasonto.owl#AffectiveState> ;

                                                     dc:description "Feelings relate more to a property associated to and object or being, than to a person’s state of mind." .



###  http://blankdots.com/open/personasonto.owl#Goals

<http://blankdots.com/open/personasonto.owl#Goals> rdf:type :Class ;

                                                   rdfs:subClassOf :Thing ;

                                                   dc:description "The goals of a Persona. The goals are divided accordingly, depending on each persona type." .



###  http://blankdots.com/open/personasonto.owl#GovermentOrganization

<http://blankdots.com/open/personasonto.owl#GovermentOrganization> rdf:type :Class ;

                                                                   rdfs:subClassOf <http://blankdots.com/open/personasonto.owl#Organization> ;

                                                                   dc:description "A governmental organization or agency. " .



###  http://blankdots.com/open/personasonto.owl#Group

<http://blankdots.com/open/personasonto.owl#Group> rdf:type :Class ;

                                                   rdfs:subClassOf <http://blankdots.com/open/personasonto.owl#Organization> ;

                                                   dc:description "Any kind of (small or large) group a person might be part of (book club)." .



###  http://blankdots.com/open/personasonto.owl#GustatoryImpairment

<http://blankdots.com/open/personasonto.owl#GustatoryImpairment> rdf:type :Class ;

                                                                 rdfs:subClassOf <http://blankdots.com/open/personasonto.owl#Disability> ;

                                                                 dc:description "Impairment of the sense of smell and taste." .



###  http://blankdots.com/open/personasonto.owl#HearingImpairment

<http://blankdots.com/open/personasonto.owl#HearingImpairment> rdf:type :Class ;

                                                               rdfs:subClassOf <http://blankdots.com/open/personasonto.owl#Disability> ;

                                                               dc:description "Conditions in which persons are fully or partially unable to detect or perceive at least some frequencies of sound." .



###  http://blankdots.com/open/personasonto.owl#Image

<http://blankdots.com/open/personasonto.owl#Image> rdf:type :Class ;

                                                   rdfs:subClassOf <http://blankdots.com/open/personasonto.owl#Resource> ;

                                                   dc:description "An image, which might be used to describe a product or person." .



###  http://blankdots.com/open/personasonto.owl#Intangible

<http://blankdots.com/open/personasonto.owl#Intangible> rdf:type :Class ;

                                                        rdfs:subClassOf <http://blankdots.com/open/personasonto.owl#Context> ;

                                                        dc:description "A context might be described by an Intangible enviroment (Operating System, Application etc. -- virtual world)." .



###  http://blankdots.com/open/personasonto.owl#IntellectualImpairment

<http://blankdots.com/open/personasonto.owl#IntellectualImpairment> rdf:type :Class ;

                                                                    rdfs:subClassOf <http://blankdots.com/open/personasonto.owl#Disability> ;

                                                                    dc:description "This type of impairment ranges from mental retardation to cognitive deficits too mild or too specific." .



###  http://blankdots.com/open/personasonto.owl#InteractiveTask

<http://blankdots.com/open/personasonto.owl#InteractiveTask> rdf:type :Class ;

                                                             rdfs:subClassOf <http://blankdots.com/open/personasonto.owl#Task> ;

                                                             :disjointWith <http://blankdots.com/open/personasonto.owl#NonInteractiveTask> ;

                                                             dc:description "InteractiveTask could be certain action performed by a person (not too often by a machine)." .



###  http://blankdots.com/open/personasonto.owl#LifeGoals

<http://blankdots.com/open/personasonto.owl#LifeGoals> rdf:type :Class ;

                                                       rdfs:subClassOf <http://blankdots.com/open/personasonto.owl#Goals> ;

                                                       dc:description "Life goals represent personal aspirations of the user that typically go beyond the context of the product being designed." .



###  http://blankdots.com/open/personasonto.owl#MediumBusiness

<http://blankdots.com/open/personasonto.owl#MediumBusiness> rdf:type :Class ;

                                                            rdfs:subClassOf <http://blankdots.com/open/personasonto.owl#Organization> ;

                                                            dc:description "A particular medium sized business or branch of an organization. ( restaurant, medical practice etc.)" .



###  http://blankdots.com/open/personasonto.owl#MentalEmotionalDisorder

<http://blankdots.com/open/personasonto.owl#MentalEmotionalDisorder> rdf:type :Class ;

                                                                     rdfs:subClassOf <http://blankdots.com/open/personasonto.owl#Disability> ;

                                                                     dc:description "Any mental disorder/illness described as a psychological or behavioral pattern associated with distress or disability that occurs in an person." .



###  http://blankdots.com/open/personasonto.owl#Mood

<http://blankdots.com/open/personasonto.owl#Mood> rdf:type :Class ;

                                                  rdfs:subClassOf <http://blankdots.com/open/personasonto.owl#AffectiveState> ;

                                                  dc:description "Are not directed at any object in particular and are experienced as more diffuse, global, and general state." .



###  http://blankdots.com/open/personasonto.owl#NGO

<http://blankdots.com/open/personasonto.owl#NGO> rdf:type :Class ;

                                                 rdfs:subClassOf <http://blankdots.com/open/personasonto.owl#Organization> ;

                                                 dc:description "A Non-Governamental Organization." .



###  http://blankdots.com/open/personasonto.owl#NegativeType

<http://blankdots.com/open/personasonto.owl#NegativeType> rdf:type :Class ;

                                                          rdfs:subClassOf <http://blankdots.com/open/personasonto.owl#PersonaType> ;

                                                          dc:description "Are used to remind and communicate, who is NOT the target of the design." .



###  http://blankdots.com/open/personasonto.owl#NonInteractiveTask

<http://blankdots.com/open/personasonto.owl#NonInteractiveTask> rdf:type :Class ;

                                                                rdfs:subClassOf <http://blankdots.com/open/personasonto.owl#Task> ;

                                                                dc:description "NonInteractiveTask could be a machine response to that action." .



###  http://blankdots.com/open/personasonto.owl#Organization

<http://blankdots.com/open/personasonto.owl#Organization> rdf:type :Class ;

                                                          rdfs:subClassOf :Thing ;

                                                          dc:description "The type of Organization a Person belongs to." .



###  http://blankdots.com/open/personasonto.owl#Participant

<http://blankdots.com/open/personasonto.owl#Participant> rdf:type :Class ;

                                                         rdfs:subClassOf <http://blankdots.com/open/personasonto.owl#Person> ;

                                                         dc:description "A person that participates in the usability test. Usualy a participant has a diresct correspondent to a Persona type." .



###  http://blankdots.com/open/personasonto.owl#Person

<http://blankdots.com/open/personasonto.owl#Person> rdf:type :Class ;

                                                    rdfs:subClassOf :Thing ;

                                                    dc:description "A person represents an individual, most likely a human." .



###  http://blankdots.com/open/personasonto.owl#Persona

<http://blankdots.com/open/personasonto.owl#Persona> rdf:type :Class ;

                                                     rdfs:subClassOf <http://blankdots.com/open/personasonto.owl#Person> ;

                                                     dc:description "As a subclass of Person, the Persona class could be regarded of being a separate identity of a person, although it has the same usage." .



###  http://blankdots.com/open/personasonto.owl#PersonaType

<http://blankdots.com/open/personasonto.owl#PersonaType> rdf:type :Class ;

                                                         rdfs:subClassOf <http://blankdots.com/open/personasonto.owl#Persona> ;

                                                         dc:description "Each Persona is classified by a PersonaType and a Person might fit into several PersonaTypes." .



###  http://blankdots.com/open/personasonto.owl#Personality

<http://blankdots.com/open/personasonto.owl#Personality> rdf:type :Class ;

                                                         :equivalentClass [ rdf:type :Restriction ;
                                                                            :onProperty <http://blankdots.com/open/personasonto.owl#influences> ;
                                                                            :someValuesFrom <http://blankdots.com/open/personasonto.owl#AffectiveState>
                                                                          ] ;

                                                         rdfs:subClassOf :Thing ;

                                                         dc:description "The MyersBriggs personality of a person." .



###  http://blankdots.com/open/personasonto.owl#Place

<http://blankdots.com/open/personasonto.owl#Place> rdf:type :Class ;

                                                   rdfs:subClassOf <http://blankdots.com/open/personasonto.owl#Context> ;

                                                   dc:description "The appropriate Place which describes the Context. " ;

                                                   rdfs:comment "A Context might only be described by a certain Place." .



###  http://blankdots.com/open/personasonto.owl#PrimaryType

<http://blankdots.com/open/personasonto.owl#PrimaryType> rdf:type :Class ;

                                                         rdfs:subClassOf <http://blankdots.com/open/personasonto.owl#PersonaType> ;

                                                         dc:description "\"The goal is to find a single persona from the set whose needs and goals can be completely and happily satisfied by a single interface without disenfranchising any of the other personas\", About Face 2.0, Cooper, pg. 71. This is the Primary Persona Type" .



###  http://blankdots.com/open/personasonto.owl#Product

<http://blankdots.com/open/personasonto.owl#Product> rdf:type :Class ;

                                                     rdfs:subClassOf <http://blankdots.com/open/personasonto.owl#Resource> ;

                                                     dc:description "A physical product or a digital product (Software Application)." .



###  http://blankdots.com/open/personasonto.owl#Resource

<http://blankdots.com/open/personasonto.owl#Resource> rdf:type :Class ;

                                                      rdfs:subClassOf :Thing ;

                                                      dc:description "Type of resources available, physical or digital." .



###  http://blankdots.com/open/personasonto.owl#Scenario

<http://blankdots.com/open/personasonto.owl#Scenario> rdf:type :Class ;

                                                      rdfs:subClassOf :Thing ;

                                                      dc:description "A series of tasks the user performs in a certain context." .



###  http://blankdots.com/open/personasonto.owl#SecondaryType

<http://blankdots.com/open/personasonto.owl#SecondaryType> rdf:type :Class ;

                                                           rdfs:subClassOf <http://blankdots.com/open/personasonto.owl#PersonaType> ;

                                                           dc:description "The secondary persona's goals and needs can mostly be met by focusing on the primary persona. Still, there are a few goals specific to them that are not a priority for the primary persona." .



###  http://blankdots.com/open/personasonto.owl#ServedType

<http://blankdots.com/open/personasonto.owl#ServedType> rdf:type :Class ;

                                                        rdfs:subClassOf <http://blankdots.com/open/personasonto.owl#PersonaType> ;

                                                        dc:description "Do not directly use the product you are designing but they are somehow affected by the system." .



###  http://blankdots.com/open/personasonto.owl#SmallBusiness

<http://blankdots.com/open/personasonto.owl#SmallBusiness> rdf:type :Class ;

                                                           rdfs:subClassOf <http://blankdots.com/open/personasonto.owl#Organization> ;

                                                           dc:description "A particular small business like a local shop or market (usually under 5 employees)." .



###  http://blankdots.com/open/personasonto.owl#SomatosensoryImpairment

<http://blankdots.com/open/personasonto.owl#SomatosensoryImpairment> rdf:type :Class ;

                                                                     rdfs:subClassOf <http://blankdots.com/open/personasonto.owl#Disability> ;

                                                                     dc:description "Insensitivity to stimuli such as touch, heat, cold, and pain." .



###  http://blankdots.com/open/personasonto.owl#SportsTeam

<http://blankdots.com/open/personasonto.owl#SportsTeam> rdf:type :Class ;

                                                        rdfs:subClassOf <http://blankdots.com/open/personasonto.owl#Organization> ;

                                                        dc:description "A sports team (football, basketball, bowling etc.)." .



###  http://blankdots.com/open/personasonto.owl#SupplementalType

<http://blankdots.com/open/personasonto.owl#SupplementalType> rdf:type :Class ;

                                                              rdfs:subClassOf <http://blankdots.com/open/personasonto.owl#PersonaType> ;

                                                              dc:description "Needs are completely satisfied by one of the primary interfaces yet they are part of the set or personas to ensure comprehensive representation." .



###  http://blankdots.com/open/personasonto.owl#Tangible

<http://blankdots.com/open/personasonto.owl#Tangible> rdf:type :Class ;

                                                      rdfs:subClassOf <http://blankdots.com/open/personasonto.owl#Context> ;

                                                      dc:description "A context might be described by an Tangible enviroment - real world." .



###  http://blankdots.com/open/personasonto.owl#Task

<http://blankdots.com/open/personasonto.owl#Task> rdf:type :Class ;

                                                  rdfs:subClassOf :Thing ;

                                                  dc:description "Tasks performed by a Person in a Scenario." .



###  http://blankdots.com/open/personasonto.owl#TechnicalGoals

<http://blankdots.com/open/personasonto.owl#TechnicalGoals> rdf:type :Class ;

                                                            rdfs:subClassOf <http://blankdots.com/open/personasonto.owl#Goals> ;

                                                            dc:description """Technical goals reflect technical aspects regarding an application/product for example: run in a variety of browser, data privacy
etc.""" .



###  http://blankdots.com/open/personasonto.owl#Time

<http://blankdots.com/open/personasonto.owl#Time> rdf:type :Class ;

                                                  rdfs:subClassOf <http://blankdots.com/open/personasonto.owl#Context> ;

                                                  rdfs:comment "A Context might only be described at a certain Time." ;

                                                  dc:description "The appropriate Time which describes the Context. " .



###  http://blankdots.com/open/personasonto.owl#UsabilityTest

<http://blankdots.com/open/personasonto.owl#UsabilityTest> rdf:type :Class ;

                                                           rdfs:subClassOf :Thing ;

                                                           dc:description "A usability test evaluates the usability of a certain Product in a Scenario." .



###  http://blankdots.com/open/personasonto.owl#VisualImpairment

<http://blankdots.com/open/personasonto.owl#VisualImpairment> rdf:type :Class ;

                                                              rdfs:subClassOf <http://blankdots.com/open/personasonto.owl#Disability> ;

                                                              dc:description "Visual impairment refers to vision loss to such a degree as a person needs an additional support to get through its surroundings." .





